#################################################################
#    Object Properties
#################################################################

###  https://schema.coypu.org/#hasEmployee
coy:hasEmployee rdf:type owl:ObjectProperty ;
                owl:inverseOf coy:isEmployeeOf ;
                rdfs:domain coy:Company ;
                rdfs:range coy:Person ;
                rdfs:comment "a person employed by a company"@en ;
                rdfs:label "hasEmployee"@en ;
                owl:equivalentProperty "https://schema.org/employee" .


###  https://schema.coypu.org/#hasImpactOn
coy:hasImpactOn rdf:type owl:ObjectProperty ;
                owl:inverseOf coy:isImpactedBy ;
                rdfs:domain coy:Event ;
                rdfs:comment "the thing impacted by an event"@en ;
                rdfs:label "hasImpactOn"@en .


###  https://schema.coypu.org/#hasLocation
coy:hasLocation rdf:type owl:ObjectProperty ;
                owl:inverseOf coy:isLocationOf ;
                rdfs:range coy:Location ;
                rdfs:comment "the location an event, company or critical infrastructure is located at"@en ;
                rdfs:label "hasLocation"@en ;
                rdfs:seeAlso "https://schema.org/location" .


###  https://schema.coypu.org/#hasProduct
coy:hasProduct rdf:type owl:ObjectProperty ;
               owl:inverseOf coy:isProductOf ;
               rdfs:domain coy:Company ;
               rdfs:range coy:Product ;
               rdfs:comment "the material, resource or service a company produces"@en ;
               rdfs:label "hasProduct"@en .


###  https://schema.coypu.org/#hasSubject
coy:hasSubject rdf:type owl:ObjectProperty ;
               owl:inverseOf coy:isSubjectOf ;
               rdfs:domain coy:Event ;
               rdfs:comment "the thing an event is about"@en ;
               rdfs:label "hasSubject"@en ;
               owl:equivalentProperty "https://schema.org/about" .


###  https://schema.coypu.org/#hasSupplier
coy:hasSupplier rdf:type owl:ObjectProperty ;
                owl:inverseOf coy:isSupplierOf ;
                rdfs:domain coy:Company ;
                rdfs:range coy:Company ;
                rdfs:comment "the company supplying a product, material, resource or service to another company"@en ;
                rdfs:label "hasSupplier"@en .


###  https://schema.coypu.org/#isActiveIn
coy:isActiveIn rdf:type owl:ObjectProperty ;
               rdfs:domain coy:Company ;
               rdfs:range coy:Industry ;
               rdfs:comment "the sector or industry a company is active in"@en ;
               rdfs:label "isActiveIn"@en .


###  https://schema.coypu.org/#isEmployeeOf
coy:isEmployeeOf rdf:type owl:ObjectProperty ;
                 rdfs:domain coy:Person ;
                 rdfs:range coy:Company ;
                 rdfs:comment "the company employing a person"@en ;
                 rdfs:label "isEmployeeOf"@en .


###  https://schema.coypu.org/#isImpactedBy
coy:isImpactedBy rdf:type owl:ObjectProperty ;
                 rdfs:range coy:Event ;
                 rdfs:comment "the event impacting a thing"@en ;
                 rdfs:label "isImpactedBy"@en .


###  https://schema.coypu.org/#isLocationOf
coy:isLocationOf rdf:type owl:ObjectProperty ;
                 rdfs:domain coy:Location ;
                 rdfs:comment "the event, company or critical infrastructure at a certain location"@en ;
                 rdfs:label "isLocationOf"@en .


###  https://schema.coypu.org/#isPartOf
coy:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:domain coy:Company ;
             rdfs:range coy:Company ;
             rdfs:comment "the concern or group a company is part of"@en ;
             rdfs:label "isPartOf"@en .


###  https://schema.coypu.org/#isProductOf
coy:isProductOf rdf:type owl:ObjectProperty ;
                rdfs:domain coy:Product ;
                rdfs:range coy:Company ;
                rdfs:comment "the company producing a material, resource or service"@en ;
                rdfs:label "isProductOf"@en .


###  https://schema.coypu.org/#isSubjectOf
coy:isSubjectOf rdf:type owl:ObjectProperty ;
                rdfs:range coy:Event ;
                rdfs:comment "the publisher or subject of an event"@en ;
                rdfs:label "isSubjectOf"@en ;
                owl:equivalentProperty "https://schema.org/subjectOf" .


###  https://schema.coypu.org/#isSupplierOf
coy:isSupplierOf rdf:type owl:ObjectProperty ;
                 rdfs:domain coy:Company ;
                 rdfs:range coy:Company ;
                 rdfs:comment "the company supplied with a product, material, resource or service by another company"@en ;
                 rdfs:label "isSupplierOf"@en .


###  https://schema.coypu.org/#isUsedIn
coy:isUsedIn rdf:type owl:ObjectProperty ;
             rdfs:domain coy:Product ;
             rdfs:range coy:Product ;
             rdfs:comment "a product, material or resource used in production of another material, resource or product"@en ;
             rdfs:label "isUsedIn"@en .


